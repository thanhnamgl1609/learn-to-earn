{
  "name": "learn-to-earn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clearCache": "rm -rf .next && npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "sequelize db:seed --seed",
    "seed:undo": "sequelize db:seed:undo --seed",
    "seed:undo:all": "sequelize db:seed:undo:all",
    "seed:create": "sequelize seed:generate --name",
    "migrate": "sequelize db:migrate",
    "migrate:db:create": "sequelize db:create",
    "migrate:create": "sequelize migration:create --name",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all",
    "redeploy:contract": "truffle deploy && npm run genContractType",
    "redeploy:migrate": "npm run drop:db && npm run create:db && npm run migrate",
    "redeploy": "npm run redeploy:contract && npm run redeploy:migrate",
    "drop:db": "sequelize db:drop",
    "create:db": "sequelize db:create",
    "genContractType": "npm run genSchoolType && npm run genNftIdentitiesType && npm run genNftCompleteCourses && npm run genNftClassRegistrationType && npm run genNftGraduationType",
    "genSchoolType": "abi-types-generator './public/contracts/School.json' --output='./types/contracts' --provider=ethers_v5",
    "genNftIdentitiesType": "abi-types-generator './public/contracts/NftIdentities.json' --output='./types/contracts' --provider=ethers_v5",
    "genNftClassRegistrationType": "abi-types-generator './public/contracts/NftClassRegistration.json' --output='./types/contracts' --provider=ethers_v5",
    "genNftCompleteCourses": "abi-types-generator './public/contracts/NftCompleteCourses.json' --output='./types/contracts' --provider=ethers_v5",
    "genNftGraduationType": "abi-types-generator './public/contracts/NftGraduation.json' --output='./types/contracts' --provider=ethers_v5"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@metamask/providers": "^10.2.1",
    "@openzeppelin/contracts": ">=4.8.2",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/moment": "^2.13.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/uuid": "^9.0.0",
    "axios": "^1.3.4",
    "ethereumjs-util": "^7.1.5",
    "ethers": "^5.7.2",
    "form-data": "^4.0.0",
    "formidable": "v3",
    "global": "^4.4.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mysql2": "^3.3.1",
    "next": "13.1.1",
    "next-iron-session": "^4.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "react-toastify": "^9.1.1",
    "sequelize": "^6.31.1",
    "swr": "^2.0.1",
    "truffle": "^5.8.0",
    "typescript": "4.9.4",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/formidable": "^2.0.5",
    "@types/lodash": "^4.14.191",
    "@types/sequelize": "^4.28.15",
    "autoprefixer": "^10.4.13",
    "ethereum-abi-types-generator": "^1.3.2",
    "postcss": "^8.4.21",
    "power-assert": "^1.6.1",
    "prettier": "^2.8.2",
    "redux-logger": "^3.0.6",
    "sequelize-cli": "^6.6.0",
    "tailwindcss": "^3.2.4"
  }
}
